#include <stdio.h>

int main() {
    // Write C code here
    int numb[1000], n, min;
    scanf("%d", &n);
    
    for(int i=0;i < n; i++){
        scanf("%d", &numb[i]);
        numb[n+i] = numb[i];
    }
    printf("\n");
    
    for(int j = 0; j < n; j++){
        for(int c = j; c < n + j; c++){
        printf("%d ", *(numb+c));
        } 
        printf("\n");
    } 

    
    return 0;
}
